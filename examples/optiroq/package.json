{
  "name": "optiroq",
  "version": "0.1.1",
  "private": true,
  "bin": {
    "basic-deployment": "bin/optiroq-app.js"
  },
  "scripts": {
    "build": "npm run build --prefix src/admin-app && tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "lint": "eslint .",
    "test": "jest",
    "test:local:convert-bom": "ts-node scripts/test-convert-bom.ts",
    "test:local:extract-data": "ts-node scripts/test-extract-data.ts",
    "test:local:map-validate": "ts-node scripts/test-map-and-validate.ts",
    "test:local:sanitize-rfq-excel": "ts-node scripts/test-sanitize-rfq-excel.ts",
    "test:local:sanitize-rfq-ocr": "ts-node scripts/test-sanitize-rfq-ocr.ts",
    "test:local:sanitize-rfq-ppt": "ts-node scripts/test-sanitize-rfq-ppt.ts",
    "test:local:validate-rfq-draft": "ts-node scripts/test-validate-and-save-rfq-draft.ts",
    "typecheck:lambdas": "tsc --project tsconfig.lambdas.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.30",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "aws-cdk": "^2.147.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@allma/admin-shell": "*",
    "@allma/core-cdk": "*",
    "@aws-sdk/client-textract": "^3.975.0",
    "@aws-sdk/s3-request-presigner": "^3.966.0",
    "@aws-sdk/util-stream-node": "^3.370.0",
    "@optiroq/convert": "1.0.0",
    "@optiroq/types": "1.0.1",
    "aws-cdk-lib": "^2.147.3",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "cdk-nag": "^2.37.55",
    "constructs": "^10.0.0",
    "exceljs": "^4.4.0",
    "pdf-parse": "^2.4.5",
    "retry": "^0.13.1",
    "source-map-support": "^0.5.21",
    "yauzl": "^3.2.0"
  }
}
